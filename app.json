{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to use them as profile pictures, You can also capture images and create a complaint request."
        }
      ]
    ],
    "name": "Colony Management",
    "slug": "ColonyManagement",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/beatle_final_logo.png",
    "splash": {
      "image": "./assets/beatle_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.BeatleAnalytics.colonymanagement",
      "adaptiveIcon": {
        "foregroundImage": "./assets/beatle_final_logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.BeatleAnalytics.colonymanagement",
      "versionCode": 12
    },
    "web": {
      "favicon": "./assets/favicon.png"
      
    }
  }
}
